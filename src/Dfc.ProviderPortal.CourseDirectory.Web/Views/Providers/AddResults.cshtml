
@{
    Layout = "_LayoutFullWidthHero";
    ViewData["Title"] = "Results for .....";

    ViewData["Target"] = "/providers/AddResults/";
    ViewData["Placeholder"] = "Enter UKPRN";
    ViewData["Area"] = "Providers";
}
<div class="cd-hero-green">
    <div class="govuk-width-container">
        <partial name="_CDHeroSearch" />
    </div>
</div>

<div class="govuk-width-container ">

    <table class="govuk-table">
        <tbody class="govuk-table__body">
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Status</th>
                <td class="govuk-table__cell">Live</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">ID</th>
                <td class="govuk-table__cell">123567890</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">UKPRN</th>
                <td class="govuk-table__cell">00000000</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Name</th>
                <td class="govuk-table__cell">Provider</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Address</th>
                <td class="govuk-table__cell"><span>this is the address</span></td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Web</th>
                <td class="govuk-table__cell">wwww.college.com</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Email</th>
                <td class="govuk-table__cell">user@college.com</td>
            </tr>
            <tr class="govuk-table__row">
                <th class="govuk-table__header" scope="row">Telephone</th>
                <td class="govuk-table__cell">00000000</td>
            </tr>
        </tbody>
    </table>
    <a href="/providers/AddResultsSuccess/" role="button" class="govuk-button">Add this Provider</a>
</div>

