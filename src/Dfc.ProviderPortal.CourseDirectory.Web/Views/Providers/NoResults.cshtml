
@{
    Layout = "_LayoutFullWidthHero";
    ViewData["Title"] = "Search for a Provider";
    ViewData["Subtext"] = "You can search for providers by entering a Name, UKPRN, City, Town or Postcode";
    ViewData["Target"] = "/providers/results/";
    ViewData["Area"] = "Providers";
    ViewData["Placeholder"] = "Search for a provider";
}

<div class="cd-hero">
    <div class="govuk-width-container">
        <partial name="_CDHeroSearch" />
    </div>
</div>
<div class="govuk-width-container">
    @*@await Component.InvokeAsync(nameof(GovukPhaseBanner))*@
    @*<vc:govuk-phase-banner is-visible="true"></vc:govuk-phase-banner>*@

    <main class="govuk-main-wrapper app-main-class" id="main-content" role="main">
        <div class="no-results">
            <h2 class="govuk-heading-m">
                No results found
                <span class="govuk-caption-m">please check that the information you have entered is correct.</span>

            </h2>
        </div>
    </main>
</div>



