
@{
    ViewData["Title"] = "Index";
}


<div class="cd-dashboard provider apprenticeship-type govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="">
            <main class="govuk-main-wrapper govuk-main-wrapper--l" id="main-content" role="main">
                <div class="govuk-grid-row">
                    <div class="">
                        <div class="govuk-panel govuk-panel--confirmation">
                            <h1 class="govuk-panel__title">
                                Delivery location deleted

                            </h1>
                            <div class="govuk-panel__body">
                                You have successfully deleted <strong>[ Location name ]</strong>from the course directory
                            </div>
                        </div>

                        <h2 class="govuk-heading-m">Next steps</h2>

                        <p class="govuk-body">
                            <a class="govuk-link" asp-controller="Apprenticeships" asp-action="classroombasedwithTable">
                                Delete another location
                            </a><br />
                            <a class="govuk-link" asp-controller="Apprenticeships" asp-action="Index">
                                Return to dashboard
                            </a>
                        </p>
                    </div>
                </div>
            </main>
        </div>
    </div>
</div>

