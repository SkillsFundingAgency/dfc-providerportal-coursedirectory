@{
    Layout = "_Layout";
    ViewData["Title"] = "Your published apprenticeship training courses";
    ViewData["Subtext"] = "Search by keyword for a training course you have published on the Course directory.";
    ViewData["Target"] = "/providers/AddResults/";
    ViewData["Placeholder"] = "Search";
    ViewData["Area"] = "Qualifications";
}
<div class="apprenticeship-search-page">
    <div>
        <h1 class="govuk-heading-l govuk-!-margin-top-4">@ViewBag.Title</h1>
        <p class="column-two-thirds govuk-body">@ViewBag.Subtext</p>
    </div>
    <div class="site-search ui-front" id="header-search" role="search">
        <div class="header-search-content govuk-!-width-two-thirds">

            <div class="search-input-wrapper govuk-!-margin-bottom-4">

                <input class="govuk-input  search-input js-search-focus" id="search" name="SearchTerm" type="search">
                <div class="cd-c-search__item cd-search__submit-wrapper">
                    <button type="submit" class=" submit">Search</button>
                </div>
            </div>
        </div>
    </div>


    <div class="govuk-grid-row">

        <div class="govuk-grid-column-two-thirds ">

            <div class="cd-apprenticeships-result govuk-body">
                <div class="govuk-!-margin-bottom-4">
                    <span>Found <strong>[Number]</strong> results for <strong>[Search term]</strong></span>
                </div>

                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">Network Engineer <span class="govuk-!-margin-left-3 govuk-tag tag-grey">Framework</span></h3>

                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">4</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>
                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">Software Developer <span class="govuk-!-margin-left-3 govuk-tag tag-grey">Framework</span></h3>
                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">4</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>
                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">
                        Aerospace Manufacturing Fitter
                    </h3>
                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">3</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>
                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">
                        Installation Electrician/Maintenance Electrician
                    </h3>
                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">3</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>
                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">
                        Power Network Craftsperson
                    </h3>
                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">3</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>
                <div class="cd-your-apprenticeships-search-result__container">
                    <h3 class="govuk-heading-m">
                        Relationship Manager (Banking)
                    </h3>
                    <p class="govuk-body">Level: <span class="govuk-!-font-weight-bold">6</span></p>
                </div>
                <div class="ctas">
                    <a class="govuk-link" href="/apprenticeships/ViewEdit">View and edit</a>
                    <a class="govuk-link" href="/apprenticeships/ConfirmApprenticeshipDelete">Delete</a>
                </div>

                <partial name="_CDPagination" />
            </div>

        </div>
    </div>
</div>